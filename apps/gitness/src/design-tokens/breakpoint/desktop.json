{
  "spacing": {
    "base": {
      "$type": "spacing",
      "$value": "{step.base}"
    },
    "3xs": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.desktop}, 1) / 2) * 2"
    },
    "2xs": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.desktop}, 1.4) / 2) * 2"
    },
    "xs": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 2) / 2) * 2"
    },
    "sm": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 2.2) / 2) * 2"
    },
    "md": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 2.5)/ 2) * 2"
    },
    "lg": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 3) / 2) * 2"
    },
    "xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 3.6) / 2) * 2"
    },
    "2xl": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.desktop}, 4) / 2) * 2"
    },
    "3xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 4.6) / 2) * 2"
    },
    "4xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 5) / 2) * 2"
    },
    "5xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.desktop}, 5.58) / 2) * 2"
    }
  },
  "breakpoint": {
    "viewport": {
      "$type": "number",
      "$value": "1440"
    },
    "viewport-min": {
      "$type": "number",
      "$value": "1080"
    },
    "viewport-max": {
      "$type": "number",
      "$value": "1919"
    },
    "container": {
      "$type": "number",
      "$value": "{spacing.2xl}"
    }
  },
  "font-size": {
    "f-3": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, -0.5))"
    },
    "f-2": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, -0.4))"
    },
    "f-1": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, -0.2))"
    },
    "f0": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 0))"
    },
    "f1": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 0.6))"
    },
    "f2": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 1))"
    },
    "f3": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 1.4))"
    },
    "f4": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 1.8))"
    },
    "f5": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.desktop}, 2.2))"
    }
  },
  "typography": {
    "interface": {
      "display": {
        "base": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.400}",
            "fontSize": "{font-size.f-3}",
            "lineHeight": "round({font-size.f-3}* {leading.normal}/2) * 2",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Large headlines, hero sections."
        },
        "strong": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.400}",
            "fontSize": "{font-size.f-3}",
            "lineHeight": "round({font-size.f-3}* {leading.normal}/2) * 2",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Large headlines with extra emphasis."
        }
      },
      "title": {
        "base": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.400}",
            "fontSize": "{font-size.f-3}",
            "lineHeight": "round({font-size.f-3}* {leading.normal}/2) * 2",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Section headers."
