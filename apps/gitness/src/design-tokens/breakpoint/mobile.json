{
  "spacing": {
    "base": {
      "$type": "spacing",
      "$value": "{step.base}"
    },
    "3xs": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.mobile}, 1) / 2) * 2"
    },
    "2xs": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.mobile}, 1.4) / 2) * 2"
    },
    "xs": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 2) / 2) * 2"
    },
    "sm": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 2.2) / 2) * 2"
    },
    "md": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 2.5)/ 2) * 2"
    },
    "lg": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 3) / 2) * 2"
    },
    "xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 3.6) / 2) * 2"
    },
    "2xl": {
      "$type": "spacing",
      "$value": "round({step.base}* pow({multiplier.mobile}, 4) / 2) * 2"
    },
    "3xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 4.6) / 2) * 2"
    },
    "4xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 5) / 2) * 2"
    },
    "5xl": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.mobile}, 5.58) / 2) * 2"
    }
  },
  "breakpoint": {
    "viewport": {
      "$type": "number",
      "$value": "360"
    },
    "viewport-max": {
      "$type": "number",
      "$value": "767"
    },
    "viewport-min": {
      "$type": "number",
      "$value": "320"
    },
    "container": {
      "$type": "number",
      "$value": "{spacing.lg}"
    }
  },
  "font-size": {
    "f-3": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, -0.6))"
    },
    "f-2": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, -0.4))"
    },
    "f-1": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, -0.2))"
    },
    "f0": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 0))"
    },
    "f1": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 0.6))"
    },
    "f2": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 0.85))"
    },
    "f3": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 1.05))"
    },
    "f4": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 1.25))"
    },
    "f5": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.mobile}, 1.4))"
    }
  },
  "typography": {
    "display": {
      "strong": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f3}",
          "lineHeight": "{line-height.lh3}",
          "letterSpacing": "{tracking.tight}",
          "textCase": "{text-case.none}"
        },
        "$description": "Large headlines with extra emphasis."
      },
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.400}",
          "fontSize": "{font-size.f2}",
          "lineHeight": "{line-height.lh2}",
          "letterSpacing": "{tracking.tight}",
          "textCase": "{text-case.none}"
        },
        "$description": "Large headlines, hero sections."
      }
    },
    "title": {
      "strong": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f1}",
          "lineHeight": "{line-height.lh1}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "Emphasized section headers."
      },
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f0}",
          "lineHeight": "{line-height.lh0}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "Section headers."
      }
    },
    "body": {
      "subtle": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.300}",
          "fontSize": "{font-size.f-1}",
          "lineHeight": "{line-height.lh-1}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "De-emphasized body text, secondary information."
      },
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.400}",
          "fontSize": "{font-size.f-1}",
          "lineHeight": "{line-height.lh-1}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "Primary text style for UI and content."
      },
      "emphasized": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f-1}",
          "lineHeight": "{line-height.lh-1}",
          "letterSpacing": "{tracking.tight}"
        },
        "$description": "Emphasized body text for important information."
      },
      "strong": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.600}",
          "fontSize": "{font-size.f-1}",
          "lineHeight": "{line-height.lh-1}",
          "letterSpacing": "{tracking.tight}"
        },
        "$description": "Strongly emphasized body text for high importance."
      }
    },
    "caption": {
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f-2}",
          "lineHeight": "{line-height.lh-2}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "Primary caption style, small but important text."
      },
      "subtle": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.400}",
          "fontSize": "{font-size.f-2}",
          "lineHeight": "{line-height.lh-2}",
          "letterSpacing": "{tracking.normal}"
        },
        "$description": "Helper text, meta information."
      },
      "data": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.default}",
          "fontWeight": "{default-font-weight-normal.500}",
          "fontSize": "{font-size.f-3}",
          "lineHeight": "{line-height.lh-3}",
          "letterSpacing": "{tracking.wide}"
        },
        "$description": "Small informational text used for displaying numerical, statistical, or technical data. "
      }
    },
    "code": {
      "base": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.mono}",
          "fontWeight": "{default-font-weight-normal.400}",
          "fontSize": "{font-size.f-1}",
          "lineHeight": "{line-height.lh-1-relaxed}",
          "letterSpacing": "{tracking.tight}"
        },
        "$description": "Code blocks and larger code snippets."
      }
    },
    "article": {
      "title": {
        "h1": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.600}",
            "fontSize": "{font-size.f2}",
            "lineHeight": "{line-height.lh2}",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Article main heading."
        },
        "h2": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.600}",
            "fontSize": "{font-size.f1}",
            "lineHeight": "{line-height.lh1-relaxed}",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Article section heading."
        },
        "h3": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.600}",
            "fontSize": "{font-size.f0}",
            "lineHeight": "{line-height.lh0-relaxed}",
            "letterSpacing": "{tracking.normal}"
          },
          "$description": "Article subsection heading."
        }
      },
      "content": {
        "lead": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.400}",
            "fontSize": "{font-size.f1}",
            "lineHeight": "{line-height.lh1-relaxed}",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Article introduction paragraph."
        },
        "paragraph": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.400}",
            "fontSize": "{font-size.f0}",
            "lineHeight": "{line-height.lh0-relaxed}",
            "letterSpacing": "{tracking.normal}"
          },
          "$description": "Article paragraph text."
        },
        "emphasized": {
          "$type": "typography",
          "$value": {
            "fontFamily": "{font-family.default}",
            "fontWeight": "{default-font-weight-normal.500}",
            "fontSize": "{font-size.f0}",
            "lineHeight": "{line-height.lh0-relaxed}",
            "letterSpacing": "{tracking.tight}"
          },
          "$description": "Emphasized paragraph text."
        }
      }
    }
  },
  "radii": {
    "sm": {
      "$type": "borderRadius",
      "$value": "{spacing.2xs}"
    }
  },
  "line-height": {
    "lh-3": {
      "$type": "lineHeights",
      "$value": "round({font-size.f-3}* {leading.snug}/2) * 2"
    },
    "lh-2": {
      "$type": "lineHeights",
      "$value": "round({font-size.f-2}* {leading.normal}/2) * 2"
    },
    "lh-1": {
      "$type": "lineHeights",
      "$value": "round({font-size.f-1}* {leading.snug}/2) * 2"
    },
    "lh-1-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.f-1}* {leading.normal}/2) * 2"
    },
    "lh0": {
      "$type": "lineHeights",
      "$value": "round({font-size.f0}* {leading.snug}/2) * 2"
    },
    "lh0-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.f0}* {leading.relaxed}/2) * 2"
    },
    "lh1": {
      "$type": "lineHeights",
      "$value": "round({font-size.f1}* {leading.tight}/2) * 2"
    },
    "lh1-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.f1}* {leading.normal}/2) * 2"
    },
    "lh2": {
      "$type": "lineHeights",
      "$value": "round({font-size.f2}* {leading.tight}/2) * 2"
    },
    "lh3": {
      "$type": "lineHeights",
      "$value": "round({font-size.f3}* {leading.tight}/2) * 2"
    },
    "lh4": {
      "$type": "lineHeights",
      "$value": "round({font-size.f4}* {leading.tight}/2) * 2"
    },
    "lh5": {
      "$type": "lineHeights",
      "$value": "round({font-size.f5}* {leading.tight}/2) * 2"
    }
  }
}