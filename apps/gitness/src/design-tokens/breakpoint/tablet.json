      "$value": "round({step.base}* pow({multiplier.tablet}, 1.4) / 2) * 2"
    },
    "3": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.tablet}, 2) / 2) * 2"
    },
    "4": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.tablet}, 2.2) / 2) * 2"
    },
    "5": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.tablet}, 2.5)/ 2) * 2"
    },
    "6": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.tablet}, 3) / 2) * 2"
    },
    "7": {
