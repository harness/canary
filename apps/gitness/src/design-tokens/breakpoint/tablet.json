    },
    "3": {
      "$type": "lineHeights",
      "$value": "round({font-size.3}* {leading.tight}/2) * 2"
    },
    "4": {
      "$type": "lineHeights",
      "$value": "round({font-size.4}* {leading.tight}/2) * 2"
    },
    "5": {
      "$type": "lineHeights",
      "$value": "round({font-size.5}* {leading.tight}/2) * 2"
    },
    "-3": {
      "$type": "lineHeights",
      "$value": "round({font-size.-3}* {leading.snug}/2) * 2"
    },
    "-2": {
      "$type": "lineHeights",
      "$value": "round({font-size.-2}* {leading.normal}/2) * 2"
    },
    "-1": {
      "$type": "lineHeights",
      "$value": "round({font-size.-1}* {leading.snug}/2) * 2"
    },
    "-1-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.-1}* {leading.normal}/2) * 2"
    },
    "0-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.0}* {leading.relaxed}/2) * 2"
    },
    "1-relaxed": {
      "$type": "lineHeights",
      "$value": "round({font-size.1}* {leading.normal}/2) * 2"
    }
