    },
    "-2": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.tablet}, -0.4))"
    },
    "-1": {
      "$type": "fontSizes",
      "$value": "round(16 * pow({multiplier.tablet}, -0.2))"
    }
