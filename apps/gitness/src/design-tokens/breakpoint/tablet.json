    },
    "11": {
      "$type": "spacing",
      "$value": "round({step.base} * pow({multiplier.tablet}, 5.58) / 2) * 2"
    },
    "base": {
      "$type": "spacing",
      "$value": "{step.base}"
    }
  },
  "breakpoint": {
    "viewport": {
      "$type": "number",
      "$value": "834"
    },
    "viewport-max": {
      "$type": "number",
      "$value": "1079"
    },
    "viewport-min": {
      "$type": "number",
      "$value": "768"
    },
    "container": {
      "$type": "number",
      "$value": "{spacing.7}"
    }
  },
  "font-size": {
    "0": {
