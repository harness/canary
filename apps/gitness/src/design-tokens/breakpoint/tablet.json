  "radii": {
    "2xs": {
      "$type": "borderRadius",
      "$value": "{spacing.base}",
      "$description": "Super small border radius for minimal rounding."
    },
    "xs": {
      "$type": "borderRadius",
      "$value": "{spacing.1}",
      "$description": "Extra small border radius for minimal rounding."
    },
    "sm": {
      "$type": "borderRadius",
      "$value": "{spacing.2}",
      "$description": "Small rounding for compact components like form controls, small buttons, and tight UI elements."
    },
    "md": {
      "$type": "borderRadius",
      "$value": "{spacing.4}",
      "$description": "Medium border radius for standard interactive elements like cards, standard buttons, and content containers. "
    },
    "lg": {
      "$type": "borderRadius",
      "$value": "{spacing.5}",
      "$description": "Large border radius for prominent UI elements like feature cards, significant content blocks, and main interactive surfaces."
    },
    "circle": {
      "$type": "borderRadius",
      "$value": "999",
      "$description": "Circular radius used for avatars, circular buttons, and other symmetrical, rounded elements that require a completely round shape."
    }
  },
  "line-height": {
    "0": {
      "$type": "lineHeights",
      "$value": "round({font-size.0}* {leading.snug}/2) * 2"
    },
    "1": {
      "$type": "lineHeights",
      "$value": "round({font-size.1}* {leading.tight}/2) * 2"
    },
    "2": {
