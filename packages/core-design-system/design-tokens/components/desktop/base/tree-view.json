      "1": {
        "$type": "spacing",
        "$value": "{tree-item.base-margin-step}*1"
      },
      "2": {
        "$type": "spacing",
        "$value": "{tree-item.base-margin-step}*2"
      },
      "3": {
        "$type": "spacing",
        "$value": "{tree-item.base-margin-step}*3"
      },
      "4": {
        "$type": "spacing",
        "$value": "{tree-item.base-margin-step}*4"
      },
      "5": {
        "$type": "spacing",
        "$value": "{tree-item.base-margin-step}*5"
      }
    },
    "base-margin-step": {
      "$type": "spacing",
      "$value": "{spacing.6}"
    }
  }
