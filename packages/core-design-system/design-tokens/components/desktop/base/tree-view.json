{
  "tree-item": {
    "height": {
      "$type": "sizing",
      "$value": "{size.8}"
    },
    "gap": {
      "$type": "spacing",
      "$value": "{spacing.1-half}"
    },
    "py": {
      "$type": "spacing",
      "$value": "{spacing.1-half}"
    },
    "pr": {
      "$type": "spacing",
      "$value": "{spacing.1-half}"
    },
    "radius": {
